{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://vercel-backend-h4dx.onrender.com/api",
    "REACT_APP_ADMIN_KEY": "admin@2024",
    "REACT_APP_RAZORPAY_KEY_ID": "rzp_test_RfKfSrxj2Nge0i"
  }
}